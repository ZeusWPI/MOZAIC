{
  "name": "bottlebats",
  "productName": "BottleBats",
  "version": "0.7.0",
  "description": "Client for the 2018 edition of the BottleBats AI competition",
  "keywords": [
    "BottleBats",
    "Zeus WPI",
    "AI",
    "MOZAIC",
    "competition",
    "electron",
    "react"
  ],
  "main": "app/dist/main.js",
  "author": {
    "name": "Zeus WPI",
    "email": "bottlebats@zeus.ugent.be",
    "url": "https://zeus.ugent.be/"
  },
  "homepage": "https://bottlebats.zeuswpi.org/",
  "bugs": {
    "url": "https://github.com/ZeusWPI/MOZAIC/issues",
    "email": "bottlebats@zeus.ugent.be"
  },
  "license": "MIT",
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZeusWPI/MOZAIC.git"
  },
  "scripts": {
    "//BUILD": "--------------------------------------------------------------",
    "build": "yarn run build-main && yarn run build-renderer",
    "build-main": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.production.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron .",
    "//DEV": "----------------------------------------------------------------",
    "dev": "yarn run dev-build-main && yarn run dev-hot-server --start-hot",
    "dev-hot-server": "cross-env NODE_ENV=development node ./webpack/devServer.js",
    "dev-hot-electron": "cross-env NODE_ENV=development HOT=1 electron .",
    "dev-build-main": "cross-env NODE_ENV=development webpack --config webpack/webpack.config.electron.js",
    "gen-docs": "webpack --config webpack/webpack.config.docs.js",
    "//CLEAN": "--------------------------------------------------------------",
    "clean-cache": "rm -r node_modules/.cache/",
    "clean-build": "rm -r app/dist/",
    "clean": "rm -r app/dist/ node_modules/",
    "//PACKAGE": "------------------------------------------------------------",
    "package": "yarn run build && build --publish never",
    "package-win": "yarn run build && build --win --x64",
    "package-linux": "yarn run build && build --linux",
    "package-mac": "yarn run build && build --mac",
    "package-all": "yarn run build && build -mwl"
  },
  "build": {
    "productName": "BottleBats",
    "appId": "org.zeuswpi.bottlebats",
    "copyright": "Copyright Â© 2018 Zeus WPI",
    "files": [
      "app/dist/",
      "node_modules/",
      "app/app.html",
      "app/main.js",
      "app/main.js.map",
      "bin/",
      "app/components/visualizer/lib/assets/",
      "resources/icon.*",
      "resources/maps/",
      "resources/bots/"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "mac": {
      "category": "public.app-category.games"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  },
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "bluebird": "^3.5.3",
    "bulma": "^0.7.2",
    "chance": "^1.0.18",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.1.0",
    "d3": "^5.5.0",
    "electron-debug": "^2.0.0",
    "electron-log": "^2.2.17",
    "electron-updater": "4.0.6",
    "electron-window-state": "^5.0.3",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "hyperscript-helpers": "^3.0.3",
    "immutable": "^3.8.2",
    "lowdb": "^1.0.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.23.0",
    "mozaic-client": "file:../../client",
    "planetwars-match-log": "file:../match-log",
    "planetwars-visualizer": "file:../visualizer",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helpers": "0.0.1",
    "react-hyperscript": "^3.2.0",
    "react-hyperscript-helpers": "^1.2.0",
    "react-jsonschema-form": "^1.0.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "source-map-support": "^0.5.9",
    "string-argv": "0.1.1",
    "tmp": "0.0.33",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "uuid": "^3.3.2"
  },
